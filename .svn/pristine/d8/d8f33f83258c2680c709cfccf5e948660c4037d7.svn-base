package com.kemean.bean;

import javax.persistence.*;

@Table(name = "daiken_goods_hot_charge")
public class DaikenGoodsHotCharge extends KemeanAbstractBaseBean {

	/**
	 * 屏位(1101一屏，1201二屏，1301三屏)
	 */
	@Column(name = "screen_position")
	private Integer screenPosition;

	/**
	 * 点击收费
	 */
	@Column(name = "click_charge")
	private Double clickCharge;

	public Integer getScreenPosition() {
		return screenPosition;
	}

	public void setScreenPosition(Integer screenPosition) {
		this.screenPosition = screenPosition;
	}

	public Double getClickCharge() {
		return clickCharge;
	}

	public void setClickCharge(Double clickCharge) {
		this.clickCharge = clickCharge;
	}

}