package com.kemean.bean;

import javax.persistence.*;

@Table(name = "daiken_order_appraisal")
public class DaikenOrderAppraisal extends KemeanAbstractBaseBean {

	/**
	 * 订单编号
	 */
	@Column(name = "order_no")
	private String orderNo;

	/**
	 * 商铺Id
	 */
	@Column(name = "id_shop")
	private Integer idShop;

	/**
	 * 用户id
	 */
	@Column(name = "id_user")
	private Integer idUser;

	/**
	 * 商品id
	 */
	@Column(name = "id_goods")
	private Integer idGoods;

	/**
	 * 用户名
	 */
	@Column(name = "user_nick_name")
	private String userNickName;

	/**
	 * 用户头像
	 */
	@Column(name = "user_head_img")
	private String userHeadImg;

	/**
	 * 1-踩 2-一般 3-赞
	 */
	@Column(name = "score")
	private Integer score;

	/**
	 * 评价内容
	 */
	@Column(name = "content")
	private String content;

	/**
	 * 评价图片
	 */
	@Column(name = "content_img")
	private String contentImg;

	/**
	 * 是否是一手商品订单
	 */
	@Column(name = "is_new_goods")
	private Boolean isNewGoods;

	public Boolean getIsNewGoods() {
		return isNewGoods;
	}

	public void setIsNewGoods(Boolean isNewGoods) {
		this.isNewGoods = isNewGoods;
	}

	public String getContentImg() {
		return contentImg;
	}

	public void setContentImg(String contentImg) {
		this.contentImg = contentImg;
	}

	public String getOrderNo() {
		return orderNo;
	}

	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}

	public Integer getIdShop() {
		return idShop;
	}

	public void setIdShop(Integer idShop) {
		this.idShop = idShop;
	}

	public Integer getIdUser() {
		return idUser;
	}

	public void setIdUser(Integer idUser) {
		this.idUser = idUser;
	}

	public Integer getIdGoods() {
		return idGoods;
	}

	public void setIdGoods(Integer idGoods) {
		this.idGoods = idGoods;
	}

	public String getUserNickName() {
		return userNickName;
	}

	public void setUserNickName(String userNickName) {
		this.userNickName = userNickName;
	}

	public String getUserHeadImg() {
		return userHeadImg;
	}

	public void setUserHeadImg(String userHeadImg) {
		this.userHeadImg = userHeadImg;
	}

	public Integer getScore() {
		return score;
	}

	public void setScore(Integer score) {
		this.score = score;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

}