<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
<meta
	http-equiv="X-UA-Compatible"
	content="IE=edge,chrome=1" />
<meta
	name="viewport"
	content="width=device-width, initial-scale=1, maximum-scale=1" />
<link
	rel="stylesheet"
	th:href="@{/layui/css/layui.css}"
	media="all" />
<link
	rel="stylesheet"
	href="https://cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.css" />
</head>
<body>
	<div
		class="layui-row"
		style="margin: 10px 0px;">
		<table
			class="layui-hide"
			id="pageData"></table>
	</div>
</body>
<script src="https://cdn.bootcss.com/jquery/1.12.4/jquery.min.js "></script>
<script src="https://cdn.bootcss.com/blueimp-md5/2.10.0/js/md5.min.js"></script>
<script
	th:src="@{/layui/layui.js}"
	charset="utf-8"></script>
<script
	th:src="@{/js/kemean_layui.js}"
	charset="utf-8"></script>
<script
	type="text/javascript"
	th:inline="javascript">
	var url_page_data = /*[[@{/shop/invest/join_user_data}]]*/;
	var url_user_record_page = /*[[@{/shop/goods/push_user_record_page}]]*/;
	var form;
	var table;
	layui.use([ 'table', 'laydate' ], function() {
		 table = layui.table;
		var laydate = layui.laydate;
		 form = layui.form;
		

		table.render({
			elem : '#pageData',
			url : url_page_data,
			cols : [ [  {
				field : 'investId',
				title : '调研编号'
			},{
				field : 'nickName',
				title : '昵称'
			}, {
				field : 'phone',
				title : '手机号'
			},{
				field : 'sexMan',
				title : '性别',
			},{
				field : 'investTypeStr',
				title : '调研类型',
			},{
				field : 'createTime',
				title : '参与时间'
			},{
				toolbar : "#operate",
				title : '操作'
			} ] ],
			page : true,
			id : 'dataTable'
		});
	});
	
	
	function behaviorRecord(objId){
		layer.open({
			type:2,
			title : "查看用户活动轨迹",
			content :url_user_record_page+'?userId='+objId,
			skin : "layui-layer-molv",
			area : [ '950px', '434px' ],
			 btn: ['关闭'],
			offset: '100px',
		})	
	}
	

</script>

<script
	type="text/html"
	id="operate">
	<button class="layui-btn layui-btn-sm" onclick="behaviorRecord({{d.objId}})"><i class="fa fa-info" aria-hidden="true"></i>&nbsp;用户活动轨迹</button> 
</script>
</html>