package com.kemean.vo.bo.admin.goods;

import java.util.List;

import com.kemean.vo.bo.admin.AdminChartBO;

public class AdminGoodsChartBO {

	/// ===========商品收入报表=======
	// 销售数量
	private List<AdminChartBO> saleNum;
	// 销售额
