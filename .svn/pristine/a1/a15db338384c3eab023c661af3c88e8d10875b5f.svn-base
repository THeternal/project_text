package com.kemean.bean;

import javax.persistence.*;

@Table(name = "daiken_goods_car")
public class DaikenGoodsCar extends KemeanAbstractBaseBean {

	/**
	 * 用户id
	 */
	@Column(name = "user_id")
	private Integer userId;

	/**
	 * 商品id
	 */
	@Column(name = "goods_id")
	private Integer goodsId;

	/**
	 * 商铺id
	 */
	@Column(name = "shop_id")
	private Integer shopId;

	/**
	 * 区分一手，二手
	 */
	@Column(name = "is_new_goods")
	private Boolean isNewGoods;

	/**
	 * 代卖商户id
	 */
	@Column(name = "id_purchasing")
	private Integer idPurchasing;

	/**
	 * SKU编号
	 */
	@Column(name = "sku_no")
	private String skuNo;

	/**
	 * 商品信息-json
	 */
	@Column(name = "record_goods_info")
	private String recordGoodsInfo;

	/**
	 * 添加数量
	 */
	@Column(name = "add_quantity")
	private Integer addQuantity;

	public Integer getShopId() {
		return shopId;
	}

	public void setShopId(Integer shopId) {
		this.shopId = shopId;
	}

	public Boolean getIsNewGoods() {
		return isNewGoods;
	}

	public void setIsNewGoods(Boolean isNewGoods) {
		this.isNewGoods = isNewGoods;
	}

	public Integer getIdPurchasing() {
		return idPurchasing;
	}

	public void setIdPurchasing(Integer idPurchasing) {
		this.idPurchasing = idPurchasing;
	}

	public Integer getAddQuantity() {
		return addQuantity;
	}

	public void setAddQuantity(Integer addQuantity) {
		this.addQuantity = addQuantity;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Integer getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(Integer goodsId) {
		this.goodsId = goodsId;
	}

	public String getSkuNo() {
		return skuNo;
	}

	public void setSkuNo(String skuNo) {
		this.skuNo = skuNo;
	}

	public String getRecordGoodsInfo() {
		return recordGoodsInfo;
	}

	public void setRecordGoodsInfo(String recordGoodsInfo) {
		this.recordGoodsInfo = recordGoodsInfo;
	}

}